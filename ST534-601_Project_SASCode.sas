PROC ARIMA DATA=data_project;
	IDENTIFY VAR=value stationarity=(adf=1);
	ESTIMATE P=2;
	ESTIMATE p=2 Q=1;
	ESTIMATE P=1 Q=1;
	ESTIMATE P=2 Q=7;
RUN;

/* Patches of inconsistant variance - Try a simple difference*/
PROC ARIMA DATA=data_project;
	IDENTIFY VAR=value(1) stationarity=(adf=1);
	ESTIMATE Q=1;
	ESTIMATE P=2 Q=1;
RUN;


/* Box-Cox Transformation suggests lambda = 0.5 transformation*/
PROC TRANSREG DATA = data_project;
	Model BOXCOX (value) = Identity(step); 
Run;
DATA data_project_transformed;
    SET data_project;
    sqrt_value = sqrt(value);
RUN;

PROC ARIMA DATA=data_project_transformed;
	IDENTIFY VAR=sqrt_value stationarity=(adf=1) NOPRINT;
	ESTIMATE P=2;
	ESTIMATE P=2 Q=1;
	ESTIMATE P=1 Q=1 NOPRINT;
	FORECAST LEAD=21;
RUN;

PROC ARIMA DATA=data_project_transformed;
	IDENTIFY VAR=transformed_value(1) stationarity=(adf=0);
	ESTIMATE Q=1;
	ESTIMATE P=2 Q=1;
	ESTIMATE P=1 Q=2;
RUN;


